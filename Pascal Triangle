using System;
using System.Collections.Generic;

namespace ConsoleApp3
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int n = int.Parse(Console.ReadLine());
            int value = 0;
            for (int i = 0; i < n; i++)
            {
                //for (int blank = 1; blank <= rows - i; blank++)
                //    Console.Write(" ");
                for (int j = 0; j <= i; j++)
                {
                    if (j == 0 || i == 0)
                        value = 1;
                    else
                        value = value * (i - j + 1) / j;
                    Console.Write(value + " ");
                }
                Console.WriteLine();
            }


        }
    }
}
